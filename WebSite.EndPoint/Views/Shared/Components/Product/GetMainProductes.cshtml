                                                                <h4 class="product_name">
                                                                    <a href="product-details.html">
                                                                        عنوان:
                                                                        @Model.bestSellers.ElementAt(i).Name
                                                                    </a>
                                                                </h4>
                                                                <div class="product_rating">
                                                                    <ul>
                                                                        @{
                                                                            int DescountNumbereateCounter = 0;
                                                                            for (int j = 0; j < 5; j++)
                                                                            {
                                                                                if (Model.bestSellers.ElementAt(i).Rate > DescountNumbereateCounter)
                                                                                {
                                                                                    <li>
                                                                                        <a href="#">
                                                                                            <i class="ion-android-star"></i>
                                                                                        </a>
                                                                                    </li>
                                                                                }
                                                                                else
