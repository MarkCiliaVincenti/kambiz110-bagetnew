                                                                        @if (Model.bestSellers.ElementAt(i).OldPrice != null)
                                                                        {
                                                                            <span class="old_price">@Model.bestSellers.ElementAt(i).OldPrice?.ToString("n0") تومان</span>
                                                                        }

                                                                        <span class="current_price">@Model.bestSellers.ElementAt(i).Price.ToString("n0") تومان</span>

