@using Application.Catalogs.CatalogItems.GetCatalogIItemPLP;
@using Application.Dtos;
@model PaginatedItemsDto<CatalogPLPDto>
@{
    var QueryStringLink = "";
    ViewData["Title"] = "لیست محصولات";
    Layout = "~/Views/Shared/_Layout.cshtml";
    int[] linkKey = new int[] { 1, 2, 3, 4, 5, 6 };
    string[] linkText = new string[] { "پربازدید", "پرفروش ترین", "محبوب ترین", "جدیدترین", "ارزان ترین", "گران ترین", };
    @foreach (var queryString in Context.Request.Query.Where(p => p.Key != "brandId").ToList())
    {

        QueryStringLink += QueryStringLink + $"{queryString.Key}={queryString.Value}&";
    }
    foreach (var queryString2 in Context.Request.Query.Where(p => p.Key != "CatalogTypeId").ToList())
    {
        QueryStringLink += QueryStringLink + $"{queryString2.Key}={queryString2.Value}&";
    }
}

